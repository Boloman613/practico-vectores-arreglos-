#include <iostream>
#include <vector>
#include <algorithm> 
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de elementos: ";
    cin >> n;

    vector<int> v(n);

    for (int i = 0; i < n; i++) {
        cout << "Elemento " << i + 1 << ": ";
        cin >> v[i];
    }

    sort(v.begin(), v.end());

    int nuevoTam = 1; 
    for (int i = 1; i < n; i++) {
        if (v[i] != v[nuevoTam - 1]) {
            v[nuevoTam] = v[i];
            nuevoTam++;
        }
    }

    v.resize(nuevoTam);

    cout << "\nVector sin duplicados:\n";
    for (int x : v) {
        cout << x << " ";
    }
    cout << endl;

    return 0;
}

