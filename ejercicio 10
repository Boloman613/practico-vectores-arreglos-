#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, m;

    cout << "Cantidad de elementos de A: ";
    cin >> n;
    cout << "Cantidad de elementos de B: ";
    cin >> m;

    vector<int> A(n), B(m);

    cout << "\nIngrese los valores de A (orden ascendente):\n";
    for (int i = 0; i < n; i++) {
        cout << "A[" << i << "]: ";
        cin >> A[i];
    }

    cout << "\nIngrese los valores de B (orden ascendente):\n";
    for (int i = 0; i < m; i++) {
        cout << "B[" << i << "]: ";
        cin >> B[i];
    }

    vector<int> C;       
    C.reserve(n + m);   

    int i = 0, j = 0;

    while (i < n && j < m) {
        if (A[i] <= B[j]) {
            C.push_back(A[i]);
            i++;
        } else {
            C.push_back(B[j]);
            j++;
        }
    }

    while (i < n) {
        C.push_back(A[i]);
        i++;
    }

    while (j < m) {
        C.push_back(B[j]);
        j++;
    }

    cout << "\nVector intercalado y ordenado:\n";
    for (int x : C) {
        cout << x << " ";
    }
    cout << endl;

    return 0;
}
