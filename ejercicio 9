#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, m;

    cout << "Ingrese el tamaño del vector A: ";
    cin >> n;
    cout << "Ingrese el tamaño del vector B: ";
    cin >> m;

    vector<int> A(n), B(m);

    cout << "\nIngrese los valores de A:\n";
    for (int i = 0; i < n; i++) {
        cout << "A[" << i << "]: ";
        cin >> A[i];
    }

    cout << "\nIngrese los valores de B:\n";
    for (int i = 0; i < m; i++) {
        cout << "B[" << i << "]: ";
        cin >> B[i];
    }

    vector<int> C;
    C.reserve(n + m); 

    for (int x : A) {
        C.push_back(x);
    }

    for (int x : B) {
        C.push_back(x);
    }

    cout << "\nVector concatenado:\n";
    for (int x : C) {
        cout << x << " ";
    }
    cout << endl;

    return 0;
}
