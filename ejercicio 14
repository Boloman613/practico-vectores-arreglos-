#include <iostream>
#include <vector>
#include <algorithm>  
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de valores: ";
    cin >> n;

    vector<float> valores(n);  

    for (int i = 0; i < n; i++) {
        cout << "Valor " << i + 1 << ": ";
        cin >> valores[i];
    }

    sort(valores.begin(), valores.end());

    cout << "\nValores ordenados ascendentemente:\n";
    for (float v : valores) {
        cout << v << " ";
    }
    cout << endl;

    return 0;
}
